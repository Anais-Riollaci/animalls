{% extends 'base.html.twig' %}{% block stylesheets %}    <link rel="stylesheet" href="{{ asset('style/gestion_modif_animal.css') }}">{% endblock %}{% block body %}    {% form_theme ModificationAnimal _self %}    {{ form_start(ModificationAnimal) }}    {{ form_row(ModificationAnimal.name) }}    {{ form_row(ModificationAnimal.species) }}    {{ form_row(ModificationAnimal.race) }}    {{ form_row(ModificationAnimal.gender) }}    {{ form_row(ModificationAnimal.birthdate) }}    {{ form_row(ModificationAnimal.category) }}    {{ form_row(ModificationAnimal.photo) }}    {% if photo is not null %}        <p>Actuellement :</p>        <img src="{{ asset(photo) }}" height="150px">    {% endif %}    {{ form_end(ModificationAnimal) }}{% endblock %}{% block javascripts %}    <script src="{{ asset('scripts/gestion_profile_animal.js') }}"></script>{% endblock %}